<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>Alexandr Sitdikov</Authors>
    <Company />
    <Description>MonetDb .NET mapi provider</Description>
    <Product>MonetDb.Mapi</Product>
    <PackageProjectUrl>https://github.com/AlexandrSitdikov/monetdb-mapi-net</PackageProjectUrl>
    <PackageIconUrl>http://dev.monetdb.org/imgs/monetdb-final-120.png</PackageIconUrl>
    <RepositoryUrl>https://github.com/AlexandrSitdikov/monetdb-mapi-net</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>database;DB;SQL;MonetDb</PackageTags>
    <Version>1.4.0</Version>
    <PackageLicenseUrl>https://raw.githubusercontent.com/AlexandrSitdikov/monetdb-mapi-net/master/LICENSE</PackageLicenseUrl>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DocumentationFile>bin\Release\MonetDb.xml</DocumentationFile>
  </PropertyGroup>

</Project>
